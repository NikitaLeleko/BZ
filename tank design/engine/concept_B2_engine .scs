concept_V2_engine => nrel_main_idtf: [engine B-2](*<- lang_en;;*);;


definition -> ...
(*
    -> rrel_key_sc_element: concept_V2_engine;;
    <= nrel_sc_text_translation:...
        (*
            -> [В-2 — советский V-образный 12-цилиндровый четырёхтактный быстроходный дизельный танковый двигатель водяного охлаждения с непосредственным впрыском (12Ч15/18).  ](*<- lang_ru;;*);;
	        -> [V-2 - Soviet V-shaped 12-cylinder, four-stroke, high-speed, water-cooled, direct-injection diesel tank engine (12Ch15/18).](*<- lang_en;;*);;
        *);;
    => nrel_main_idtf: [Опр.(двигатель В-2)](*<- lang_ru;;*);;
    => nrel_main_idtf: [Def.(engine V-2)](*<- lang_en;;*);;
*);;

