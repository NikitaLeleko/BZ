concept_Minsk_destroyer_cannon => nrel_main_idtf: [«Minsk» destroyer cannon](*<- lang_en;;*);;


definition -> ...
(*
    -> rrel_key_sc_element: concept_Minsk_destroyer_cannon;;
    <= nrel_sc_text_translation:...
        (*
            -> [Основным вооружением  эсминца «Минск» являются артиллерийские установки, размещающающиеся <b><sc_element sys_idtf="concept_deck">палубах</b>.](* <- lang_ru;; => nrel_format: format_html;; *);;
	        -> [The main weapons of the «Minsk» destroyer are artillery mounts located on <b><sc_element sys_idtf="concept_deck">decks</b>.](* <- lang_en;; => nrel_format: format_html;; *);;
        *);;
    => nrel_main_idtf: [Опр.(Вооружение эсминца «Минск»)](*<- lang_ru;;*);;
    => nrel_main_idtf: [Def.(«Minsk» destroyer cannon)](*<- lang_en;;*);;
*);;

concept_Minsk_destroyer_cannon<-rrel_key_sc_element:...(*
	<-illustration;;
	=>nrel_main_idtf:[Изобр.(Вооружение эсминца «Минск»)](*<-lang_ru;;*);;
	<=nrel_sc_text_translation:...
