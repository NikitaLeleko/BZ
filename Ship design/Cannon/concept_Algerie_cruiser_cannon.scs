        (*
            -> [Основным вооружением  крейсера «Алжир» являются артиллерийские установки, размещающающиеся <b><sc_element sys_idtf="concept_deck">палубах</b> и <b><sc_element sys_idtf="concept_sponson">спонсонах</b>](* <- lang_ru;; => nrel_format: format_html;; *);;
	        -> [The main weapons of the Algeri cruiser are artillery mounts located on <b><sc_element sys_idtf="concept_deck">decks</b> and <b><sc_element sys_idtf="concept_sponson">sponsons</b>.](* <- lang_en;; => nrel_format: format_html;; *);;
        *);;
    => nrel_main_idtf: [Опр.(Вооружение крейсера «Алжир»)](*<- lang_ru;;*);;
    => nrel_main_idtf: [Def.(«Algerie» cruiser cannon)](*<- lang_en;;*);;
*);;

