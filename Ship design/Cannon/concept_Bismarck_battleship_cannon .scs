concept_Bismarck_battleship_cannon => nrel_main_idtf: [«Bismarck» battleship cannon](*<- lang_en;;*);;


definition -> ...
(*
    -> rrel_key_sc_element: concept_Bismarck_battleship_cannon;;
    <= nrel_sc_text_translation:...
        (*
            -> [Основным вооружением  линкора «Бисмарк» являются артиллерийские установки, размещающающиеся <b><sc_element sys_idtf="concept_deck">палубах</b> и <b><sc_element sys_idtf="concept_sponson">спонсонах</b>](* <- lang_ru;; => nrel_format: format_html;; *);;
	        -> [The main weapons of the «Bismarck» battleship are artillery mounts located on <b><sc_element sys_idtf="concept_deck">decks</b> and <b><sc_element sys_idtf="concept_sponson">sponsons</b>.](* <- lang_en;; => nrel_format: format_html;; *);;
        *);;
    => nrel_main_idtf: [Опр.(Вооружение линкора «Бисмарк»)](*<- lang_ru;;*);;
    => nrel_main_idtf: [Def.(«Bismarck» battleship cannon)](*<- lang_en;;*);;
*);;

