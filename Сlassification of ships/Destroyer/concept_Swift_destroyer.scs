concept_Swift_destroyer=> nrel_main_idtf: [Destroyer "Swift"](*<- lang_en;;*);;


definition -> ...
(*
    -> rrel_key_sc_element: concept_Swift_destroyer;;
    <= nrel_sc_text_translation:...
        (*
            -> [Свифт — британский эсминец. По своим габаритам и вооружению он приблизился к крейсерам.](*<- lang_ru;;*);;

	        -> [Swift is a british destroyer. In terms of its dimensions and armament, it approached the cruisers.](*<- lang_en;;*);;
        *);;
    => nrel_main_idtf: [Опр.(Эсминец «Свифт»)](*<- lang_ru;;*);;
    => nrel_main_idtf: [Def.(Destroyer "Swift")](*<- lang_en;;*);;
*);;

concept_Swift_destroyer<-rrel_key_sc_element:...(*
	<-illustration;;
	=>nrel_main_idtf:[Изобр.(Эсминец «Свифт»)](*<-lang_ru;;*);;
	<=nrel_sc_text_translation:...
	(*
    	->rrel_example:
